{
  "manifest_version": 3,
  "name": "GroqBot WhatsApp Extension",
  "version": "1.0.0",
  "description": "WhatsApp extension for GroqBot integration",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["index.js"]
    }
  ]
}
